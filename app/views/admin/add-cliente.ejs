                        <div class="card">
                            <div class="card-header">
                                <h5 class="title">Novo cliente</h5>
                            </div>
                            <div class="card-body">
															<hr>
															<h6 class="muted">Informações básicas</h6>
                                <form action="/clientes/salvar" method="POST">
                                    <div class="row">
                                        <div class="col-md-3 pr-1">
                                            <div class="form-group">
                                                <label>CNPJ</label>
                                                <input type="text" class="form-control" placeholder="Digite o CNPJ" name="cnpj" id="cnpj"/>
                                            </div>
                                        </div>
                                        <div class="col-md-5 px-1">
                                            <div class="form-group">
                                                <label>Razao social</label>
                                                <input type="text" class="form-control" placeholder="Digite a razão social" id="razaoSocial" name="razao_social"/>
                                            </div>
                                        </div>
                                     		<div class="col-md-4 pl-1">
                                            <div class="form-group">
                                                <label for="exampleInputEmail1">IE</label>
                                                <input type="text" class="form-control" placeholder="Digite a inscrição estadual" id="inscricaoEstadual" name="inscricao_estadual"/>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-4 pr-1">
                                            <div class="form-group">
                                                <label>Nome fantasia</label>
                                                <input type="text" class="form-control" placeholder="Digite o nome fantasia" id="nomeFantasia" name="nome_fantasia" />
                                            </div>
                                        </div>
                                        <div class="col-md-3 px-1">
                                                    <div class="form-group">
                                                            <label>CEP</label>
                                                            <input type="text" class="form-control" placeholder="Digite o CEP" id="cep" name="cep"/>
                                                    </div>
                                            </div>
                                        <div class="col-md-5 pl-1">
                                            <div class="form-group">
                                                    <label>Endereço</label>
                                                    <input type="text" class="form-control" placeholder="Digite o endereço" value="Av. 9 de julho, 123" id="enderecoEntrega" name="endereco_entrega"/>
                                            </div>
                                        </div>
                                    </div>
                                    <hr>
                                    <div>
                                        <h6>Informações do comprador</h6>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-8 pr-1">
                                            <div class="form-group">
                                                    <label>Nome do comprador</label>
                                                    <input type="email" class="form-control" placeholder="Digite o nome do comprador" id="nomeComprador" name="nome_comprador" />
                                            </div>
                                        </div>
                                        <div class="col-md-4 pl-1">
                                            <div class="form-group">
                                                    <label>E-mail</label>
                                                    <input type="email" class="form-control" placeholder="Digite seu e-mail" id="email" name="email" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-4 pr-1">
                                            <div class="form-group">
                                                    <label>Telefone (FIXO)</label>
                                                    <input type="text" class="form-control" placeholder="Digite seu telefone" id="telefoneFixo" name="telefone_fixo" />
                                            </div>
                                        </div>
                                        <div class="col-md-4 px-1">
                                            <div class="form-group">
                                                <label>Whatsapp</label>
                                                <input type="text" class="form-control" placeholder="Digite seu whatsapp" id="whatsapp" name="whatsapp" />
                                            </div>
	                                      </div>
                                            <div class="col-md-4 pl-1">
                                                <div class="form-group">
                                                    <label>E-mail (XML)</label>
                                                    <input type="email" class="form-control" placeholder="Digite seu e-mail XML para NF" id="emailxml" name="email_xml" />
                                                </div>
	                                      </div>
                                	  </div>
                                  	<div class="row">
                                        <div class="col-md-3 pr-1">
                                            <div class="form-group">
                                                <label>Prazo de pagamento</label>
                                                <input type="tel" class="form-control" placeholder="Informe o prazo de pagamento" id="prazoPgamento" name="prazo_pagamento"/>
                                            </div>
                                        </div>
                                        <div class="col-md-2 pr-1">
                                                <div class="form-group">
                                                        <label>Valor caixa</label>
                                                        <input type="text" class="form-control" placeholder="R$ 000,00" id="valorCaixa" name="valor_caixa"/>
                                                </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <label>Observações</label>
                                                <textarea rows="4" cols="80" class="form-control" placeholder="OBS." id="observacoes" name="observacoes"></textarea>
                                            </div>
                                            <input class="btn btn-success btn-md" type="submit" value="salvar"/>
                                        </div>
                                    </div>
                                </form>
